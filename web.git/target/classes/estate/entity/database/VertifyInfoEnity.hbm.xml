<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.VertifyInfoEnity" table="vertifyinfo" schema="" catalog="PropertySystem">
        <id name="phone">
            <column name="phone" sql-type="varchar" not-null="true"/>
        </id>
        <property name="id">
            <column name="id" sql-type="varchar" />
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" />
        </property>
        <property name="status">
            <column name="status" sql-type="tinyint" />
        </property>

        <property name="imageIdList">
            <column name="image_id_list" sql-type="varchar" length="50"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>